<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/head'); %>
</head>

<body>
    <h4>Recent Bundles</h4>
    <% bundles.forEach(function(bundle) { %>
        <div>
            <span>
                <%= bundle.proposal.blockNumber %>
            </span>
            <span>
                <%= bundle.status %>
            </span>
            <a href="https://etherscan.io/tx/<%= bundle.proposal.transactionHash %>">
                <%= bundle.proposal.transactionHash %>
            </a>
        </div>
    <% }); %>
</body>

</html>